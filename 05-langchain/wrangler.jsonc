{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "hello-langchain",
	"main": "src/worker.py",
	"compatibility_date": "2025-11-02",
	"compatibility_flags": [
		"python_workers"
	],
	"rules": [
		{
			"type": "Data",
			"globs": ["python_modules/**/*.js"],
			"fallthrough": true
		}
	],
	"observability": {
		"enabled": true
	}
}
