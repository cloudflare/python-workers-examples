{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "python-durable-object",
	"main": "src/entry.py",
	"compatibility_date": "2025-11-02",
	"compatibility_flags": [
		"python_workers"
	],
	"durable_objects": {
		"bindings": [
			{
				"name": "LISTS",
				"class_name": "List"
			}
		]
	},
	"migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "List"
      ]
    }
  ],
	"observability": {
		"enabled": true
	}
}
